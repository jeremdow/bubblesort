{#
/**
 * @file vector.html.twig
 * Default theme implementation to present Vector data.
 *
 * This template is used when viewing Vector pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_vector()
 *
 * @ingroup themeable
 */
#}
<article{{ attributes.addClass('vector') }}>
  <table>
    <thead>
      <tr><th>i</th><th>value</th></tr>
    </thead>
    {% for i in vector.field_array %}
    <tr>
      <td>{{ loop.index }}</td><td>{{ i.value }}</td>
    </tr>
    {% endfor %}
  </table>
  <table>
    {% for i in vector.field_array %}
    <tr>
      <td>
        <svg width="100%" height="10">
          <rect width="{{ i.value }}%" height="10" style="fill:rgb(255,0,0)"/>
        </svg>
      </td>
    </tr>
    {% endfor %}
  </table>
</article>
